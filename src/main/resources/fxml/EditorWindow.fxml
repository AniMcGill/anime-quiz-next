<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.input.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="240.0" prefWidth="480.0"
      xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.msac.quiz.EditorController">
   <children>
      <GridPane alignment="CENTER_LEFT">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="BOTTOM" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Question">
                <font>
                    <Font size="16.0" />
                </font>
            </Label>

            <Label text="Answer" GridPane.rowIndex="1">
                <font>
                    <Font size="16.0" />
                </font>
            </Label>
             <TextField fx:id="answerTextField" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="1" />

            <Label text="Points" GridPane.rowIndex="2">
                <font>
                    <Font size="16.0" />
                </font>
            </Label>

             <Label text="Timing" GridPane.rowIndex="3">
                 <font>
                    <Font size="16.0" />
                </font>
             </Label>
             <Label text="Answered" GridPane.rowIndex="4">
                 <font>
                    <Font size="16.0" />
                </font>
             </Label>

            <Button fx:id="deleteButton" onAction="#deleteButton_Click" mnemonicParsing="false" text="Delete" GridPane.rowIndex="6" />
            <HBox alignment="BOTTOM_RIGHT" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="3" GridPane.columnSpan="2" GridPane.rowIndex="6">
               <children>
                  <Button fx:id="saveButton" onAction="#saveButton_Click" mnemonicParsing="false" text="Save">
                     <HBox.margin>
                        <Insets right="5.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="cancelButton" onAction="#cancelButton_Click" mnemonicParsing="false" text="Cancel" />
               </children>
            </HBox>
         </children>
      </GridPane>
   </children>
</VBox>
